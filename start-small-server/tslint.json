{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "rules": {
      "quotemark": [
          true,
          "single"
      ],
      "object-literal-sort-keys": false,
      "no-console": [
          true,
          "debug",
          "info",
          "time",
          "timeEnd",
          "trace"
      ],
      "arrow-return-shorthand": [true, "multiline"]
    },
  "rulesDirectory": [],
  "jsRules": {}
}